{
  "name": "{ projectName }",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack serve --open --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "android": "cordova run android --device",
    "android:build": "npm run build & npm run android",
    "android:install": "cordova platform add android",
    "test": "cross-env NODE_ENV=test mocha --require @babel/register --recursive test/unit",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=lcovonly --reporter=text npm test",
    "test:coveralls": "npm run test:coverage && cat ./coverage/lcov.info | coveralls"
  },
  "sass": "src/index",
  "main": "src/index",
  "cordova": {
    "platforms": [
      "android"
    ],
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-headercolor": {}
    }
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/plugin-transform-runtime": "latest",
    "@babel/preset-env": "latest",
    "@babel/register": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-plugin-istanbul": "latest",
    "babel-plugin-transform-html-import-to-string": "latest",
    "chai": "latest",
    "cordova": "latest",
    "cordova-android": "latest",
    "cordova-res": "latest",
    "coveralls": "latest",
    "cross-env": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-plugin-import": "latest",
    "html-webpack-plugin": "latest",
    "mini-css-extract-plugin": "latest",
    "mocha": "latest",
    "mocha-lcov-reporter": "latest",
    "node-sass": "latest",
    "nyc": "latest",
    "raw-loader": "latest",
    "sass-loader": "latest",
    "sinon": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "appable": "latest",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-headercolor": "^1.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "jsdom": "latest"
  }
}