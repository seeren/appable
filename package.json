{
  "name": "babel-skeleton",
  "description": "Babel skeleton for build es6 JavaScript in the browser",
  "version": "1.0.4",
  "keywords": [
    "babel",
    "es6",
    "javascript",
    "skeleton"
  ],
  "homepage": "https://github.com/seeren/babelskeleton",
  "author": "Cyril Ichti",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "test": "mocha --require babel-register --recursive test/unit",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --recursive test/unit",
    "test:coveralls": "npm run test:coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "sass": "src/index.scss",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/seeren/babel-skeleton"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-plugin-istanbul": "latest",
    "babel-plugin-transform-html-import-to-string": "latest",
    "babel-preset-env": "latest",
    "browser-sync": "latest",
    "browser-sync-webpack-plugin": "latest",
    "chai": "latest",
    "coveralls": "latest",
    "cross-env": "latest",
    "css-loader": "latest",
    "jsdom": "latest",
    "mini-css-extract-plugin": "latest",
    "mocha": "latest",
    "mocha-lcov-reporter": "latest",
    "node-sass": "latest",
    "nyc": "latest",
    "sass-loader": "latest",
    "sinon": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "engines": {
    "node": ">=8"
  }
}
