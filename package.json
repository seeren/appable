{
  "name": "app-js",
  "description": "App-js is a JavaScript Library for build user interfaces and scale web applications",
  "version": "0.0.1",
  "keywords": [
    "application",
    "component",
    "route",
    "hybrid"
  ],
  "author": "Cyril Ichti",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "mocha --require @babel/register --recursive test/unit",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --recursive test/unit",
    "test:coveralls": "npm run test:coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "main": "dist/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "app": "cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seeren/app-js"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/register": "latest",
    "babel-loader": "latest",
    "babel-plugin-istanbul": "latest",
    "chai": "latest",
    "coveralls": "latest",
    "cross-env": "latest",
    "jsdom": "latest",
    "mocha": "latest",
    "mocha-lcov-reporter": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "webpack": "latest",
    "webpack-cli": "latest"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "npm": "latest"
  }
}